<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>E Voting System</display-name>
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>uni.bamberg.appengine.auth.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>uni.bamberg.appengine.auth.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CandidateServlet</servlet-name>
		<servlet-class>uni.bamberg.appengine.frontend.CandidateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CandidateServlet</servlet-name>
		<url-pattern>/candidate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CandidateViewServlet</servlet-name>
		<servlet-class>uni.bamberg.appengine.frontend.CandidateViewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CandidateViewServlet</servlet-name>
		<url-pattern>/candidateview</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TimingServlet</servlet-name>
		<servlet-class>uni.bamberg.appengine.frontend.TimingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TimingServlet</servlet-name>
		<url-pattern>/timing</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>StudentServlet</servlet-name>
		<servlet-class>uni.bamberg.appengine.frontend.StudentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentServlet</servlet-name>
		<url-pattern>/student</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>StudentViewServlet</servlet-name>
		<servlet-class>uni.bamberg.appengine.frontend.StudentViewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentViewServlet</servlet-name>
		<url-pattern>/viewstudent</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TimingViewServlet</servlet-name>
		<servlet-class>uni.bamberg.appengine.frontend.TimingViewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TimingViewServlet</servlet-name>
		<url-pattern>/timingview</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>VotingViewServlet</servlet-name>
		<servlet-class>uni.bamberg.appengine.frontend.VotingViewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VotingViewServlet</servlet-name>
		<url-pattern>/votingview</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>VotingServlet</servlet-name>
		<servlet-class>uni.bamberg.appengine.frontend.VotingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VotingServlet</servlet-name>
		<url-pattern>/voting</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>ApplicationFilter</filter-name>
		<filter-class>uni.bamberg.appengine.auth.ApplicationFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ApplicationFilter</filter-name>
		<url-pattern>/</url-pattern>
	</filter-mapping>
	<filter>
		<filter-name>LogoutFilter</filter-name>
		<filter-class>uni.bamberg.appengine.auth.LogoutFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LogoutFilter</filter-name>
		<url-pattern>/logout</url-pattern>
	</filter-mapping>
</web-app>